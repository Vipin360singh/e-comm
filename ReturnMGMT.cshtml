@{
    ViewBag.Title = "ReturnMGMT";
    Layout = "~/Views/Shared/AdminMasterPage.cshtml";
}

@model List <e_comm.Content.Models.TBL_ReturnOrder>

<table class="table table-bordered" style="background:linear-gradient(white,pink)">
    <tr>
        <th>Order Id</th>
        <th>Return to product</th>
        <th>Email </th>
        
    </tr> 
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem=>item.OrderId)</td>
            <td>@Html.DisplayFor(modelItem=>item.Return_To)</td>
            <td>@Html.DisplayFor(modelItem=>item.Email)</td>
            
        </tr>
    }
</table>